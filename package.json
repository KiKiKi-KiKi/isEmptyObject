{
  "name": "is-empty-object",
  "version": "0.0.1",
  "description": "Check a parameter is empty Object. Array, Date, Class, Function, Regex, Symoble are return false.",
  "main": "dist-cjs/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KiKiKi-KiKi/isEmptyObject.git"
  },
  "keywords": [
    "empty",
    "Object",
    "empty Object"
  ],
  "author": "KiKiKi KiKi <https://twitter.com/KiKiKi_KiKi>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/KiKiKi-KiKi/isEmptyObject/issues"
  },
  "scripts": {
    "build:cjs": "tsc --project . --module commonjs --outDir ./dist-cjs",
    "build:esm": "tsc --project . --module esNext --outDir ./dist-esm",
    "build": "npm run build:cjs && npm run build:esm",
    "test": "jest",
    "test:w": "jest --runInBand --watch",
    "test:coverage": "jest --runInBand --coverage",
    "lint": "eslint \"src/**/*.{js,ts}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,ts}\"",
    "format": "prettier --write \"src/**/*.{js,ts}\""
  },
  "homepage": "https://github.com/KiKiKi-KiKi/isEmptyObject#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "coveralls": "^3.1.0",
    "eslint": "*",
    "eslint-config-prettier": "*",
    "eslint-plugin-prettier": "*",
    "jest": "*",
    "prettier": "*",
    "ts-jest": "*",
    "ts-node": "*",
    "typescript": "*"
  }
}
